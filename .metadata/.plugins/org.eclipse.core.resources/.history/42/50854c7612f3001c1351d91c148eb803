// button callback
void HAL_GPIO_EXTI_Callback(uint16_t GPIO_Pin)
{
	int i = 0;
	if(GPIO_Pin == BTN_Pin)
	{
		i = 1;
	}
	if (GPIO_Pin == BTN_2_Pin)
	{
		i = 2;
	}

