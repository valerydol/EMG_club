#ifndef INC_CLOCK_H_
#define INC_CLOCK_H_

struct Clock_
{
	int tickCounetInMS;
};



//void initClock();
//void incrementClock();
//

void initClock()
{
	clock.tickCounetInMS = 0;
}

void incrementClock()
{
	clock.tickCounetInMS++;
}


