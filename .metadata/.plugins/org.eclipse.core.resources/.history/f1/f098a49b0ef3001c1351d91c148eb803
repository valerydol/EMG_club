#include "clock.h"
static struct ClockTimer clockTimer ;


void initClock()
{
	clockTimer.btn_number=BTN_NON;
	clockTimer.tickCounter = 0;
	clockTimer.tickIn = 0;
	clockTimer.tickOut = 0;
}

void incrementClock()
{
	clockTimer.tickCounter++;
}
