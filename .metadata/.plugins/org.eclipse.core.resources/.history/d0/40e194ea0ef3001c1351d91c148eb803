#ifndef INC_CLOCK_H_
#define INC_CLOCK_H_

typedef enum
{
	BTN_NON = 0,
	BTN_1 ,
	BTN_2
}BTN_NUMBER;

struct ClockTimer{

	BTN_NUMBER btn_number;
	int tickCounter;
	int tickIn;
	int tickOut;
};


void initClock();
void incrementClock();
void setBtnNum(BTN_NUMBER btnNum);




#endif /* INC_CLOCK_H_ */
